	<filter>
		<shadow id='bg' distance='5' angle='10' alpha='40' blurX='10' blurY='10' inner='true' />
		<shadow id='title'  distance='3' angle='45' alpha='100' blurX='8' blurY='8' inner='true' knockout='true' />
		<shadow id='data' distance='3' angle='40' alpha='35' blurX='10' blurY='10' />
		<shadow id='low' distance='2' angle='45' alpha='35' blurX='5' blurY='5' />
		<bevel id='data' angle='2' blurX='20' blurY='5' distance='10' highlightAlpha='10' highlightColor='ffffff' shadowAlpha='10' />
		<bevel id='tooltip' angle='45' blurX='10' blurY='10' distance='5' highlightAlpha='50' shadowAlpha='20' />
	</filter>
	
	<legend shadow='low' x='1' y='-200' width='380' height='90' margin='5' fill_color='ffffff' fill_alpha='60' line_color='000000' line_alpha='0' line_thickness='0' layout='horizontal' size='20' color='444466' alpha='95' />
