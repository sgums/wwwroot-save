<chart>

	<axis_value shadow='low' size='25' max='100' color='000000' alpha='50' steps='5' prefix='' suffix='' decimals='0' separator='' show_min='true' />
	<axis_category shadow='low'  size='30' />
	<chart_border color='000000' top_thickness='0' bottom_thickness='3' left_thickness='0' right_thickness='0' />
	<chart_label position='middle' size='30' />
	<chart_data>
		<row>
			<null/>
			<string>State %</string>			
		</row>
		<row>
			<string>Hold</string>
			<number shadow='data' bevel='data' label='20' >20</number>			
		</row>
		<row>
			<string>On ACW</string>
			<number shadow='data' bevel='data' label='20'>20</number>
		</row>
		<row>
			<string>Available</string>
			<number shadow='data' bevel='data' label='20'>20</number>
		</row>
		<row>
			<string>On ACD</string>
			<number shadow='data' bevel='data' label='20'>20</number>
		</row>
		<row>
			<string>In Aux</string>
			<number shadow='data' bevel='data' label='20'>20</number>
		</row>
	</chart_data>
	<chart_grid_h alpha='20' color='000000' thickness='1' type='solid' />
	<chart_grid_v alpha='20' color='000000' thickness='1' type='dashed' />
	<chart_rect shadow='bg' x='70' y='-90' width='280' height='500' positive_color='eeeeff' negative_color='ff4400' positive_alpha='90' negative_alpha='75' />
	<!-- <chart_transition type='drop' delay='0' duration='3' order='series' /> -->
	<chart_type>stacked column</chart_type>

	<!--
	<draw>
		<text shadow='title' transition='slide_up' delay='1' duration='0.5' color='000033' alpha='25' rotation='-90' size='64' x='0' y='298' width='300' height='50' h_align='right' v_align='middle'>GROWTH</text>
		<text shadow='low' transition='slide_up' delay='1.3' duration='0.7' color='ffffff' alpha='50' rotation='-90' size='25' x='32' y='303' width='300' height='50' h_align='right' v_align='middle'>report</text>
		<image transition='dissolve' delay='1.5' url='images/full_screen.swf' x='93' y='28' width='20' height='15' alpha='80' />
